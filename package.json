{
  "name": "ijn-boilerplate-pro",
  "version": "2.0.2",
  "private": true,
  "author": "Jirayu Ninlapun",
  "homepage": "https://TheIceJI.com",
  "scripts": {
    "lint": "yarn prettier && yarn eslint",
    "eslint": "yarn eslint-resource && yarn eslint-page",
    "prettier": "yarn prettier-resource && yarn prettier-page",
    "eslint-resource": "eslint --fix-dry-run \"./resources/views/**/**/**/**/*.{js,ts,jsx,tsx}\" --ext jsconfig.json",
    "eslint-page": "eslint --fix-dry-run \"./pages/**/**/**/*.{js,ts,jsx,tsx}\" --ext jsconfig.json",
    "prettier-resource": "prettier --list-different \"./resources/views/**/**/**/**/*.{js,ts,jsx,tsx}\"",
    "prettier-page": "prettier --list-different \"./pages/**/**/**/*.{js,ts,jsx,tsx}\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "export": "EXPORT=true next build && EXPORT=true next export",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze-b": "cross-env BUNDLE_ANALYZE=both next build",
    "post": "next-sitemap --config sitemap.config.js"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@tailwindcss/typography": "^0.5.2",
    "@use-gesture/react": "^10.2.11",
    "axios": "^0.26.1",
    "framer-motion": "^6.3.0",
    "graphql": "^16.3.0",
    "graphql-request": "^4.2.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^12.1.5",
    "next-pwa": "^5.5.0",
    "next-share": "^0.13.0",
    "nprogress": "^0.2.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-intersection-observer": "^8.33.1",
    "sharp": "^0.30.3",
    "zustand": "^3.7.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@emotion/babel-plugin": "^11.9.2",
    "@next/bundle-analyzer": "^12.1.5",
    "@types/node": "^17.0.24",
    "@types/nprogress": "^0.2.0",
    "@types/react": "^18.0.5",
    "@types/tailwindcss": "^3.0.10",
    "@typescript-eslint/eslint-plugin": "^5.19.0",
    "@typescript-eslint/parser": "^5.19.0",
    "autoprefixer": "^10.4.4",
    "eslint": "^8.13.0",
    "eslint-config-next": "^12.1.5",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-tailwindcss": "^3.5.0",
    "file-loader": "^6.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-sitemap": "^2.5.20",
    "postcss": "^8.4.12",
    "prettier": "^2.6.2",
    "prettier-plugin-tailwindcss": "^0.1.8",
    "tailwindcss": "^3.0.24",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.72.0"
  }
}
