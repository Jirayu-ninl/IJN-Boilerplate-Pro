{
  "name": "i-framework-pro",
  "version": "1.0.0",
  "private": true,
  "author": "Jirayu Ninlapun",
  "homepage": "https://TheIceJI.com",
  "yarn-upgrade-all": {
    "ignore": [
      "react",
      "react-dom"
    ]
  },
  "scripts": {
    "lint": "yarn prettier && yarn eslint",
    "eslint": "eslint --fix \"./src/**/*.{js,jsx}\" --ext jsconfig.json",
    "prettier": "prettier --list-different \"./src/**/*.{js,jsx,md}\"",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "server": "NODE_ENV=production node server.js",
    "export": "EXPORT=true next build && EXPORT=true next export",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze-b": "cross-env BUNDLE_ANALYZE=both next build"
  },
  "dependencies": {
    "@emotion/css": "^11.7.1",
    "@emotion/react": "^11.7.1",
    "@tailwindcss/typography": "^0.5.1",
    "@types/nprogress": "^0.2.0",
    "@types/tailwindcss": "^3.0.7",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "framer-motion": "^6.2.4",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "next": "^12.0.10",
    "nprogress": "^0.2.0",
    "react": "18.0.0-rc.0-next-a3bde7974-20220208",
    "react-dom": "18.0.0-rc.0-next-a3bde7974-20220208",
    "react-intersection-observer": "^8.33.1",
    "react-use-gesture": "^9.1.3",
    "smooth-scrollbar": "^8.7.4"
  },
  "devDependencies": {
    "@babel/core": "^7.17.2",
    "@emotion/babel-plugin": "^11.7.2",
    "@next/bundle-analyzer": "^12.0.10",
    "@types/node": "^17.0.17",
    "@types/react": "^17.0.39",
    "autoprefixer": "^10.4.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-tailwind": "^0.2.1",
    "file-loader": "^6.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-offline": "^5.0.5",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.68.0"
  }
